{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev local",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "kebapp_flutter/lib/main.dart",
      "args": ["--dart-define=SERVER_URL=http://<host ip>:8080/"]
    },
    {
      "name": "dev server",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "kebapp_flutter/lib/main.dart",
      "args": ["--dart-define-from-file=.env"]
    },
    {
      "name": "prod",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "kebapp_flutter/lib/main.dart",
      "args": ["--dart-define-from-file=.env"]
    }
  ]
}
